{
  "connection": "default",
  "collectionName": "category",
  "info": {
    "name": "category",
    "description": "Categories for blog posts"
  },
  "options": {
    "timestamps": true
  },
  "attributes": {
    "name": {
      "default": "",
      "unique": true,
      "type": "string",
      "required": true
    },
    "description": {
      "default": "",
      "type": "string",
      "required": true
    },
    "post": {
      "model": "post",
      "via": "category"
    },
    "slug": {
      "default": "",
      "unique": true,
      "type": "string",
      "required": true
    }
  }
}