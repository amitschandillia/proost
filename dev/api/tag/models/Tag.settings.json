{
  "connection": "default",
  "collectionName": "",
  "info": {
    "name": "tag",
    "description": "Tags for blog articles"
  },
  "options": {
    "timestamps": true
  },
  "attributes": {
    "name": {
      "default": "",
      "unique": true,
      "type": "string",
      "required": true
    },
    "description": {
      "default": "",
      "unique": true,
      "type": "string",
      "required": true
    },
    "posts": {
      "collection": "post",
      "via": "tags"
    }
  }
}